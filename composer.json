{
    "name": "ricardosierra/robo-docker-compose",
    "description": "Docker compose for Robo Task Runner",
    "license": "MIT",
    "keywords": ["docker", "compose", "robo", "task runner"],
    "type": "robo-tasks",
    "require": {
        "php": ">=5.5.0",
        "consolidation/robo": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Droath\\RoboDockerCompose\\": "./src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "droath/robo-docker-compose":"*"
    }
}
